---
import MainLayout from "../layouts/MainLayout.astro";
import { Image } from 'astro:assets';
import brev from "../assets/images/brev.png";
import laas from "../assets/images/laas.png";
---
<MainLayout title="Titel">
  <article class="bg-black min-h-screen flex items-center justify-center relative">
    <Image src={brev} alt="" class="max-w-full max-h-full" />
    <Image src={laas} alt="" class="absolute top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2 max-w-full max-h-full" />
  </article>
</MainLayout>